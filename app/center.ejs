<div id="switchcontainer">
  <div style="position:absolute;top:0;left:0;width:<%- getWidth(425) %>;z-index:99;pointer-events: none;height:<%- getHeight(80) %>;opacity:0" id="vol">
    <div style="position:absolute;top:0;left:0;z-index:-1;border:none;outline:none;background-color:#232323;width:<%- getWidth(425) %>;height:<%- getHeight(80) %>;opacity:<%- 200/255 %>"/>
      <svg xmlns="http://www.w3.org/2000/svg" version="1.0" width="<%- getWidth(50) %>" height="<%- getHeight(50) %>" viewBox="0 0 <%- getWidth(75) %> <%- getHeight(75) %>" style="position:absolute;z-index:1;top:50%;left:<%- getWidth(25) %>; transform: translate(0, -50%)">
        <path d="M39.389,13.769 L22.235,28.606 L6,28.606 L6,47.699 L21.989,47.699 L39.389,62.75 L39.389,13.769z" style="stroke:#fff;stroke-width:5;stroke-linejoin:round;fill:#fff;"></path>
        <path d="M48,27.6a19.5,19.5 0 0 1 0,21.4M55.1,20.5a30,30 0 0 1 0,35.6M61.6,14a38.8,38.8 0 0 1 0,48.6" style="fill:none;stroke:#fff;stroke-width:5;stroke-linecap:round"></path>
      </svg>
      <input type="button" style="position:absolute;height:<%- getHeight(10) %>;width:<%- getWidth(300) %>;background-color:#858585;outline:none;border:none;border-radius:<%- getHeight(10) %>px;top:50%;left:<%- getWidth(95) %>;transform: translate(0, -50%)"/>
      <div style="position:absolute;top:50%;left:<%- getWidth(95) %>;overflow: hidden;transform: translate(0, -50%);height:<%- getHeight(10) %>;width:300" id="setvol">
        <input type="button" style="position:absolute;top:0;left:0;height:<%- getHeight(10) %>;width:<%- getWidth(300) %>;background-color:#ffffff;outline:none;border:none;border-radius:10px;"/>
      </div>
    </div>
  </div>
</div>
