<img width="1280" height="720" style="position:absolute;top: 0; left: 0" src="<%- defaulticon.background %>">
<p style="position: absolute;top: <%- uijson["startup_menu"]["info_text"]["y"] %>; left: <%- uijson["startup_menu"]["info_text"]["x"] %>;border: 0;margin: 0px 0px;background-color: #000;font-family: 'Font'; font-size: 25; background-color: transparent; border: 0; color: <%- uijson["text_color"] %>;visibility: <%- uijson["startup_menu"]["info_text"]["visible"] %>;"><%- lang["startup_welcome_info"]+"<br>"+lang["startup_control_info"] %></p>
<div style="position: relative;top: <%- uijson["startup_menu"]["users_menu_item"]["y"] %>; left: <%- uijson["startup_menu"]["users_menu_item"]["x"] %>;width: 880;height:500;overflow-x: hidden;overflow-y: auto;margin: 0px 0px;padding: 0px" type="button" id="users">
  <%
    let num = undefined;
    let n = -1;
    let userss = users.map((user) => {
      n += 1
      return `<div style="position:absolute;top:${eval(n*100)};left:0;width:880;height:100"><img width="100" height="100" style="position:absolute;top: 0;z-index: 1; left: 25" src="${(user.usericon === "default") ? path.join(__dirname, "ulaunch", "User.png") : user.usericon}"><p style="position: absolute;top: 50%;transform:translate(0, -50%);z-index: 1; left: 140;margin: 0px 0px;border: 0;font-family: 'Font'; font-size: 25; background-color: transparent; border: 0; color: ${uijson["text_color"]};">${user.username} ${(user.password) ? lang["startup_password"] : ""}</p><input type="button" style="position:absolute;top:0;left:0;z-index: 0;width: 880;height:100;outline:none;border: none;background-color:${(n == 0) ? uijson["menu_focus_color"] : uijson["menu_bg_color"]};" id="${eval(n)}g"/><input type="button" style="position:absolute;top:0;left:0;z-index: 2;outline: none;width: 880;height:100;border: none;background-color:transparent" id="${eval(n)}"/></div>`
    });
    n += 1;
    userss.push(`<div style="position:absolute;top:${eval(n*100)};left:0;width:880;height:100"><p style="position: absolute;top: 50%;transform:translate(0, -50%);z-index: 1; left: 25;border: 0;font-family: 'Font'; font-size: 25; background-color: transparent;margin: 0px 0px; border: 0; color: ${uijson["text_color"]};">${lang["startup_new_user"]}</p><input type="button" style="position:absolute;top:0;left: 0;;z-index: 0;width: 880;height:100;outline:none;border: none;background-color:${(n == 0) ? uijson["menu_focus_color"] : uijson["menu_bg_color"]};" id="${n}g"/><input type="button" style="position:absolute;top:0;left:0;z-index: 2;outline: none;width: 880;height:100;border: none;background-color:transparent" id="${eval(n)}" alt="create"/>`);
    userss.join("");
  %>
  <%- userss %>
</div>
