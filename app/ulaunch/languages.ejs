<img width="1280" height="720" style="position:absolute;top: 0; left: 0" src="<%- defaulticon.background %>">
<p style="position: absolute;top: <%- uijson["languages_menu"]["info_text"]["y"] %>;left: 50%;transform: translate(-50%, 0);border: 0;margin: 0px 0px;background-color: #000;font-family: 'Font'; font-size: 25; background-color: transparent; border: 0; color: <%- uijson["text_color"] %>;visibility: <%- uijson["languages_menu"]["info_text"]["visible"] %>;"><%- lang["lang_info_text"] %></p>
<div style="position: relative;top: <%- uijson["languages_menu"]["languages_menu_item"]["y"] %>; left: <%- uijson["languages_menu"]["languages_menu_item"]["x"] %>;width: 880;height:400;overflow-x: hidden;overflow-y: auto;margin: 0px 0px;padding: 0px;visibility: <%- uijson["languages_menu"]["languages_menu_item"]["visible"] %>;" type="button" id="settings">
  <%
    let n = -1;
    let langs = "";
    let keys = Object.keys(Languages);
    for(var i=0; i<keys.length; i++){
      let key = keys[i];
      let langu = Languages[key];
      n += 1;
      langs += `<div style="position:absolute;top:${eval(n*100)};left:0;width:880;height:100"><p style="position: absolute;top: 50%;transform:translate(0, -50%);z-index: 1; left: 25;margin: 0px 0px;border: 0;font-family: 'Font'; font-size: 25; background-color: transparent; border: 0; color: ${uijson["text_color"]};">${langu}${(key === testersettings.lang) ? ` ${lang["lang_selected"]}` : ""}</p><input type="button" style="position:absolute;top:0;left:0;z-index: 0;width: 880;height:100;outline:none;border: none;background-color:${(n == 0) ? uijson["menu_focus_color"] : uijson["menu_bg_color"]};" id="${eval(n)}g"/><input type="button" style="position:absolute;top:0;left:0;z-index: 2;outline: none;width: 880;height:100;border: none;background-color:transparent" id="${eval(n)}" alt="${key}"/></div>`
    }
  %>
  <%-
    langs
  %>
</div>
